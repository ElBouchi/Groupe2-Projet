<Window x:Class="Projet.View.executeWork"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Projet"
        mc:Ignorable="d"
        Title="executeWork" Height="450" Width="826" Loaded="executeWork_Loaded">
    <Grid>
        <DataGrid x:Name="Works" Margin="149,21,149,158" LoadingRow="Works_LoadingRow"/>
        <Button x:Name="sequentialExec" Content="Sequential Execution" HorizontalAlignment="Left" Margin="552,329,0,0" VerticalAlignment="Top" Height="37" Width="125" Click="sequentialExec_Click"/>
        <Button x:Name="uniqueExec" Content="Running a single backup job" HorizontalAlignment="Left" Margin="218,329,0,0" VerticalAlignment="Top" Width="180" Height="37" Click="uniqueExec_Click"/>
        <TextBox x:Name="index" HorizontalAlignment="Left" Margin="147,333,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="48" Height="33"/>
        <Label Content="Row of the backup work" HorizontalAlignment="Left" Margin="10,336,0,0" VerticalAlignment="Top" Height="30" Width="137"/>
    </Grid>
</Window>
