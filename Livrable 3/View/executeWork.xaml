<Window x:Class="Projet.View.executeWork"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Projet"
        mc:Ignorable="d"
        Title="executeWork" Height="550" Width="950" ResizeMode="NoResize" Background="#404040"  Loaded="executeWork_Loaded">
    <Grid>
        <DataGrid x:Name="Works" Margin="149,10,149,179" LoadingRow="Works_LoadingRow" HorizontalGridLinesBrush="Orange" VerticalGridLinesBrush="Orange" BorderBrush="Orange" FontSize="14" />
        <Button x:Name="sequentialExec" HorizontalAlignment="Left" Margin="530,397,0,0" VerticalAlignment="Top" Height="51" Width="254" Click="sequentialExec_Click" Style="{StaticResource btnHover}" FontSize="15" Background="Orange" Foreground="#404040"/>
        <Button x:Name="uniqueExec" HorizontalAlignment="Left" Margin="183,397,0,0" VerticalAlignment="Top" Width="254" Height="50" Click="uniqueExec_Click" Style="{StaticResource btnHover}" FontSize="15" Background="Orange" Foreground="#404040"/>
    </Grid>
</Window>
